{
  "manifest_version": 3,
  "name": "PlanMyTimetable Capture",
  "description": "Capture timetable data from allocate+ (mytimetable) sites to use on PlanMyTimetable.",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"]
  },
  "web_accessible_resources": [
    {
      "resources": ["bookmarklet.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "action": {
    "default_icon": {
      "48": "images/icon-deactive.svg",
      "96": "images/icon-deactive.svg"
    }
  },
  "icons": {
    "48": "images/icon.svg",
    "96": "images/icon.svg"
  },
  "permissions": ["scripting", "activeTab", "tabs"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      }
    }
  }
}
